{
  "name": "Pais",
  "plural": "pais",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "DisableAllMethods": {
      "expose": [
        "find",
        "__get__estados"
      ]
    }
  },
  "properties": {
    "nome": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "estados": {
      "type": "hasMany",
      "model": "Estado",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
