{
  "name": "Filial",
  "plural": "filials",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [
      {
        "relation": "endereco",
        "scope": {
          "include": [
            "enderecoTipo"
          ]
        }
      },
      {
        "relation": "telefones",
        "scope": {
          "include": [
            "telefoneTipo"
          ]
        }
      }
    ]
  },
  "mixins": {
    "DisableAllMethods": {
      "expose": [
        "findById",
        "find",
        "create",
        "updateAttributes",
        "exists",
        "count"
      ]
    }
  },
  "properties": {
    "matriz": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "instituicao": {
      "type": "belongsTo",
      "model": "Instituicao",
      "foreignKey": ""
    },
    "endereco": {
      "type": "belongsTo",
      "model": "Endereco",
      "foreignKey": ""
    },
    "telefones": {
      "type": "hasAndBelongsToMany",
      "model": "Telefone",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "findById"
    }
  ],
  "methods": {}
}
