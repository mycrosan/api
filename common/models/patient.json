{
  "name": "Patient",
  "plural": "patients",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": {
      "relation": "appointments",
      "scope": {
        "include": ["physician"]
      }
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "physicians": {
      "type": "hasMany",
      "model": "Physician",
      "foreignKey": "patientId",
      "through": "Appointment"
    },
    "appointments": {
      "type": "hasMany",
      "model": "Appointment",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
