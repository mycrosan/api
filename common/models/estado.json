{
  "name": "Estado",
  "plural": "estados",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "DisableAllMethods": {
      "expose": [
        "__get__municipios"
      ]
    }
  },
  "properties": {
    "nome": {
      "type": "string",
      "required": true
    },
    "sigla": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "pais": {
      "type": "belongsTo",
      "model": "Pais",
      "foreignKey": ""
    },
    "municipios": {
      "type": "hasMany",
      "model": "Municipio",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
