{
  "name": "Usuario",
  "plural": "usuarios",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [
      {
        "relation": "enderecos",
        "scope": {
          "include": [
            "enderecoTipo"
          ]
        }
      },
      {
        "relation": "telefones",
        "scope": {
          "include": [
            "telefoneTipo"
          ]
        }
      }
    ]
  },
  "properties": {
    "imagem": {
      "type": "string"
    },
    "nome": {
      "type": "string",
      "required": true
    },
    "dtNascimento": {
      "type": "date",
      "required": true
    },
    "sexo": {
      "type": "string",
      "required": true
    },
    "dtCriacao": {
      "type": "date",
      "required": true
    },
    "dtAlteracao": {
      "type": "date"
    },
    "dtRemocao": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "filial": {
      "type": "belongsTo",
      "model": "Filial",
      "foreignKey": ""
    },
    "enderecos": {
      "type": "hasAndBelongsToMany",
      "model": "Endereco",
      "foreignKey": ""
    },
    "telefones": {
      "type": "hasAndBelongsToMany",
      "model": "Telefone",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
